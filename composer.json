{
    "name": "swoole-bundle/z-engine",
    "version": "8.4.0",
    "description": "Library that provides direct access to native PHP structures",
    "type": "library",
    "require": {
        "php": "8.4.*",
        "ext-ffi": "*"
    },
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Alexander Lisachenko",
            "email": "lisachenko.it@gmail.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "ZEngine\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "ZEngine\\" : "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
